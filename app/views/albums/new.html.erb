<h1>Create new album</h1>
<div class="row">
  <div class="col-md-12">
    <%= form_for(@album, :html => {:multipart => true}) do |f| %>
      
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.submit "Create my album", class: "btn btn-default" %>

    <% end %>
  </div>
</div>
